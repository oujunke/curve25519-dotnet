{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/Fe:",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "/I",
                "${workspaceFolder}\\android\\jni\\ed25519\\",
                "/I",
                "${workspaceFolder}\\android\\jni\\ed25519\\nacl_includes",
                "/I",
                "${workspaceFolder}\\android\\jni\\ed25519\\additions",
                "/I",
                "${workspaceFolder}\\android\\jni\\ed25519\\additions\\generalized",
                "/I",
                "${workspaceFolder}\\android\\jni\\ed25519\\tests",
                "${workspaceFolder}\\android\\jni\\ed25519\\*.c",
                "${workspaceFolder}\\android\\jni\\ed25519\\additions\\*.c",
                "${workspaceFolder}\\android\\jni\\ed25519\\additions\\generalized\\*.c",
                "${workspaceFolder}\\android\\jni\\ed25519\\nacl_sha512\\*.c",
                "${workspaceFolder}\\android\\jni\\ed25519\\tests\\*.c"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
